{
	"env": {
		"browser": true,
		"es6": true,
		"es2021": true
	},
	"extends": "airbnb",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      }
    }
  },
	"rules": {
		"import/no-unresolved": "off",
		"import/prefer-default-export": "off",
		"import/no-extraneous-dependencies": "off",
		"no-param-reassign": ["error", {
      "props": true,
      "ignorePropertyModificationsFor": [
        "state"
      ]
    }]
	}
}
